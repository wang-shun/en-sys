  <form:scripts>// 获取角色类型树&#xD;
getRoleTypeTree:function(){&#xD;
	return $view.roleTypeTree;&#xD;
},&#xD;
// 获取角色列表&#xD;
getRoleGroup:function(){&#xD;
	return $view.roleGroup;&#xD;
},&#xD;
// 数据源加载前&#xD;
page_beforeDataSourceLoad:function($params){&#xD;
	$model.search = {};	&#xD;
},
// 页面加载完成
page_onload:function(){
	var _this = this;&#xD;
	var doit = debounce(function(search){&#xD;
			if(_this.getRoleGroup()){&#xD;
				_this.getRoleGroup().refresh(search);&#xD;
			}&#xD;
		},300,false);	&#xD;
		&#xD;
	$scope.$watch(&quot;$model.search&quot;,function(v,o){	&#xD;
		if(angular.isUndefined(v)){&#xD;
			return ;&#xD;
		}else if(v.roleName==o.roleName &#xD;
				&amp;&amp; v.roleDesc==o.roleDesc&#xD;
				&amp;&amp; v.roleType==o.roleType&#xD;
				&amp;&amp; v.roleUsage==o.roleUsage){&#xD;
			return;&#xD;
		}&#xD;
		doit(v);&#xD;
	},true);
},
// 角色类型树点击
roleTypeTree_nodeClick:function(event,treeId,treeNode,clickFlag){
	if(angular.isUndefined($model.search)){&#xD;
		$model.search = {};&#xD;
	}&#xD;
	$model.search.roleType = treeNode.id;	&#xD;
	$scope.$apply();&#xD;
},
// 新增角色类型
addRoleType:function(){
	var _this = this;&#xD;
	Modal.open('f/editRoleType',{type:&quot;add&quot;,roleTypeId:&quot;&quot;},function(result){&#xD;
		if('success'==result){&#xD;
			_this.refreshRoleType();&#xD;
	    }&#xD;
	});	
},
// 编辑角色类型
editRoleType:function(){
	var selectNodes = this.getRoleTypeTree().getSelectedNodes();&#xD;
	if(angular.isArray(selectNodes) &amp;&amp; selectNodes.length>0){&#xD;
		if(selectNodes.length>1){&#xD;
			Messenger.post({&#xD;
			    'message': &quot;请仅选择一个需要编辑的角色类型！&quot;,&#xD;
			    'type': 'error',&#xD;
			});&#xD;
			return;&#xD;
		}&#xD;
		var selectNode = selectNodes[0]; &#xD;
		if(angular.isUndefined(selectNode) || angular.isUndefined(selectNode.id)){&#xD;
			Messenger.post({&#xD;
			    'message': &quot;请选择需要编辑的角色类型！&quot;,&#xD;
			    'type': 'error',&#xD;
			});&#xD;
		}else if(&quot;0&quot;==selectNode.id){&#xD;
			Messenger.post({&#xD;
			    'message': selectNode.name+&quot;不允许被编辑！&quot;,&#xD;
			    'type': 'error',&#xD;
			});&#xD;
		}else{&#xD;
			var _this = this;&#xD;
			Modal.open('f/editRoleType',{type:&quot;edit&quot;,roleTypeId:selectNode.id},function(result){&#xD;
				if('success'==result){&#xD;
			    	_this.refreshRoleType();&#xD;
			    }&#xD;
			});	&#xD;
		}	&#xD;
	}else{&#xD;
		Messenger.post({&#xD;
		    'message': &quot;请选择需要编辑的角色类型！&quot;,&#xD;
		    'type': 'error',&#xD;
		});&#xD;
	}
},
// 删除角色类型
delRoleType:function(){
	var selectNodes = this.getRoleTypeTree().getSelectedNodes();&#xD;
	if(angular.isArray(selectNodes) &amp;&amp; selectNodes.length>0){&#xD;
		var roleTypeIds = [];&#xD;
		var roleTypeNames = &quot;&quot;;&#xD;
		var _this = this;&#xD;
		var delErrMess = &quot;&quot;;&#xD;
		angular.forEach(selectNodes, function (value, key) {&#xD;
				roleTypeIds.push(value.id);&#xD;
				roleTypeNames += value.name+&quot;,&quot;;&#xD;
				if(&quot;0&quot;==value.id){&#xD;
					delErrMess += value.name+&quot;,&quot;;&#xD;
				}&#xD;
        	});&#xD;
        if(delErrMess.length>0){&#xD;
        	Messenger.post({&#xD;
	                'message': delErrMess.substring(0,delErrMess.length-1) + &quot;不允许被删除！&quot;,&#xD;
	                'type': 'error',&#xD;
	            });&#xD;
	        return;&#xD;
        }&#xD;
        Modal.openDelConfirm({message:&quot;是否确认删除角色类型及其所属角色? &lt;br>待删除角色类型：&quot;+roleTypeNames.substring(0, roleTypeNames.length-1)},function(){&#xD;
        	$http.post(&quot;ws/delRoleType&quot;,{&quot;roleTypeIds&quot;:roleTypeIds})&#xD;
        			.success(function(data, status, headers, config){&#xD;
						Messenger.post({&#xD;
			                'message': &quot;删除角色类型成功！&quot;,&#xD;
			                'type': 'success',&#xD;
			            });&#xD;
			            _this.refreshRoleType();&#xD;
			           $model.search.roleType = &quot;&quot;;&#xD;
			           $scope.$apply();&#xD;
					});&#xD;
        });		&#xD;
	}else{&#xD;
		Messenger.post({&#xD;
		    'message': &quot;请选择需要删除的机构！&quot;,&#xD;
		    'type': 'error',&#xD;
		});&#xD;
	}
},
// 刷新角色类型
refreshRoleType:function(){&#xD;
	this.getRoleTypeTree().reAsyncChildNodes(null,&quot;refresh&quot;,false);
},&#xD;
// 新增角色&#xD;
addRole:function(){&#xD;
	var _this = this;&#xD;
	var roleType = $model.search.roleType;&#xD;
	if(angular.isUndefined(roleType) || null==roleType){&#xD;
		roleType = &quot;&quot;;&#xD;
	}&#xD;
	Modal.open('f/addRole',{type:&quot;add&quot;,&quot;roleType&quot;:roleType,roleId:&quot;&quot;},function(result){&#xD;
		if('success'==result){&#xD;
	    	_this.refreshRole();&#xD;
	    }&#xD;
	});&#xD;
},&#xD;
// 编辑角色 &#xD;
editRole:function(){&#xD;
	var selectNodes = this.getRoleGroup().getSelectDatas();&#xD;
	if(angular.isArray(selectNodes) &amp;&amp; selectNodes.length>0){&#xD;
		if(selectNodes.length>1){&#xD;
			Messenger.post({&#xD;
			    'message': &quot;请仅选择一个需要编辑的角色！&quot;,&#xD;
			    'type': 'error',&#xD;
			});&#xD;
			return;&#xD;
		}&#xD;
		var selectNode = selectNodes[0]; &#xD;
		if(angular.isUndefined(selectNode) || angular.isUndefined(selectNode.roleId)){&#xD;
			Messenger.post({&#xD;
			    'message': &quot;请选择需要编辑的角色！&quot;,&#xD;
			    'type': 'error',&#xD;
			});&#xD;
		}else if(&quot;true&quot;==selectNode.isUD){&#xD;
			var _this = this;&#xD;
			Modal.open('f/editRole',{type:&quot;edit&quot;,&quot;roleType&quot;:&quot;&quot;,roleId:selectNode.roleId},function(result){&#xD;
				if('success'==result){&#xD;
			    	_this.refreshRole();&#xD;
			    }&#xD;
			});				&#xD;
		}else{&#xD;
			Messenger.post({&#xD;
			    'message': selectNode.roleName+&quot;不允许被编辑！&quot;,&#xD;
			    'type': 'error',&#xD;
			});&#xD;
		}&#xD;
	}else{&#xD;
		Messenger.post({&#xD;
		    'message': &quot;请选择需要编辑的角色！&quot;,&#xD;
		    'type': 'error',&#xD;
		});&#xD;
	}&#xD;
},&#xD;
// 删除角色&#xD;
delRole:function(){&#xD;
	var selectNodes = this.getRoleGroup().getSelectDatas();&#xD;
	if(angular.isArray(selectNodes) &amp;&amp; selectNodes.length>0){&#xD;
		var roleIds = [];&#xD;
		var roleNames = &quot;&quot;;&#xD;
		var _this = this;&#xD;
		var delErrMess = &quot;&quot;;&#xD;
		angular.forEach(selectNodes, function (value, key) {&#xD;
				roleIds.push(value.roleId);&#xD;
				roleNames += value.roleName+&quot;,&quot;;&#xD;
				if(&quot;true&quot;!=value.isUD){&#xD;
					delErrMess += value.roleName+&quot;,&quot;;&#xD;
				}&#xD;
        	});&#xD;
        if(delErrMess.length>0){&#xD;
        	Messenger.post({&#xD;
	                'message': delErrMess.substring(0,delErrMess.length-1) + &quot;不允许被删除！&quot;,&#xD;
	                'type': 'error',&#xD;
	            });&#xD;
	        return;&#xD;
        }&#xD;
        Modal.openDelConfirm({message:&quot;是否确认删除角色?&lt;br>待删除角色：&quot;+roleNames.substring(0, roleNames.length-1)},function(){&#xD;
        	$http.post(&quot;ws/delRole&quot;,{&quot;roleIds&quot;:roleIds})&#xD;
        			.success(function(data, status, headers, config){&#xD;
						Messenger.post({&#xD;
			                'message': &quot;删除角色成功！&quot;,&#xD;
			                'type': 'success',&#xD;
			            });&#xD;
			            _this.refreshRole();&#xD;
					});&#xD;
        });		&#xD;
	}else{&#xD;
		Messenger.post({&#xD;
		    'message': &quot;请选择需要删除的角色！&quot;,&#xD;
		    'type': 'error',&#xD;
		});&#xD;
	}&#xD;
},
// 设置权限
roleSetRes:function(){
	var roleIds = [];&#xD;
	var roleNames = &quot;&quot;;	&#xD;
	var _this = this;&#xD;
	var delErrMess = &quot;&quot;;&#xD;
	var selectNodes = this.getRoleGroup().getSelectDatas();&#xD;
	if(angular.isArray(selectNodes) &amp;&amp; selectNodes.length>0){		&#xD;
		angular.forEach(selectNodes, function (value, key) {&#xD;
				roleIds.push(value.roleId);&#xD;
				roleNames += value.roleName+&quot;,&quot;;&#xD;
				if(value.isAdmin==&quot;true&quot;){&#xD;
					delErrMess += value.roleName+&quot;,&quot;;&#xD;
				}&#xD;
        	});        	&#xD;
	}else{&#xD;
		Messenger.post({&#xD;
		    'message': &quot;请选择需要设置权限的角色！&quot;,&#xD;
		    'type': 'error',&#xD;
		});&#xD;
		return;&#xD;
	}&#xD;
	if(delErrMess.length>0){&#xD;
        	Messenger.post({&#xD;
	                'message': delErrMess.substring(0,delErrMess.length-1) + &quot;不能设置资源！&quot;,&#xD;
	                'type': 'error',&#xD;
	            });&#xD;
	        return;&#xD;
    }&#xD;
    Modal.open('f/roleSetRes',{&quot;roleIds&quot;:roleIds,&quot;roleNames&quot;:roleNames.substring(0, roleNames.length-1)},function(result){&#xD;
				if('success'==result){&#xD;
			    	_this.refreshRole();&#xD;
			    }&#xD;
			});
},
// 分配机构
roleSetOrg:function(){
	var roleIds = [];&#xD;
	var roleNames = &quot;&quot;;	&#xD;
	var _this = this;&#xD;
	var isEveryone = false;&#xD;
	var everyoneName = &quot;&quot;;&#xD;
	var selectNodes = this.getRoleGroup().getSelectDatas();&#xD;
	if(angular.isArray(selectNodes) &amp;&amp; selectNodes.length>0){		&#xD;
		angular.forEach(selectNodes, function (value, key) {&#xD;
				roleIds.push(value.roleId);&#xD;
				roleNames += value.roleName+&quot;,&quot;;&#xD;
				if(value.isEveryone==&quot;true&quot;){&#xD;
					isEveryone = true;&#xD;
					everyoneName = value.roleName;&#xD;
				}&#xD;
        	});        	&#xD;
	}else{&#xD;
		Messenger.post({&#xD;
		    'message': &quot;请选择需要设置权限的角色！&quot;,&#xD;
		    'type': 'error',&#xD;
		});&#xD;
		return;&#xD;
	}&#xD;
	if(isEveryone){&#xD;
    	Messenger.post({&#xD;
                'message': everyoneName+ &quot;已分配给每一用户！&quot;,&#xD;
                'type': 'error',&#xD;
            });&#xD;
        return;&#xD;
    }&#xD;
    Modal.open('f/roleSetOrg',{&quot;roleIds&quot;:roleIds,&quot;roleNames&quot;:roleNames.substring(0, roleNames.length-1)},function(result){&#xD;
				if('success'==result){&#xD;
			    	_this.refreshRole();&#xD;
			    }&#xD;
			});
},
// 分配用户
roleSetUser:function(){
	var roleIds = [];&#xD;
