            });&#xD;
        return;&#xD;
    }&#xD;
    Modal.open('f/roleSetUser',{&quot;roleIds&quot;:roleIds,&quot;roleNames&quot;:roleNames.substring(0, roleNames.length-1)},function(result){&#xD;
				if('success'==result){&#xD;
			    	_this.refreshRole();&#xD;
			    }&#xD;
			});
},&#xD;
// 刷新角色&#xD;
refreshRole:function(){&#xD;
	this.getRoleGroup().refresh($model.search);&#xD;
}</form:scripts>
