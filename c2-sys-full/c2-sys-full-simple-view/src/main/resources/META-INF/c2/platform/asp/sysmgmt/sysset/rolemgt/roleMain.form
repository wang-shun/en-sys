  <form:datasources>
    <form:datasource xsi:type="form:Inputs"/>
    <form:datasource xsi:type="form:TreeDataSource" id="roleTypeTree" name="角色类型树型数据源" impl="com.chinacreator.asp.sysmgmt.sysset.rolemgt.RoleTypeTreeContentProviderImpl" type="custom"/>
    <form:datasource xsi:type="form:PagingDataSource" id="RoleList" name="角色列表型数据源" logic="com.chinacreator.asp.sysmgmt.sysset.rolemgt.RoleDataArrayContentProviderImpl" pageSize="10" type="custom"/>
