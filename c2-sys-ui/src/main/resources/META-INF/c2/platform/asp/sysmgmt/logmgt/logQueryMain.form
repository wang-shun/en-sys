  <form:scripts>// 日志列表Tab
getLogGroup:function(){
	return $view.logGroup;
},
logDetails:function(){
	var selectNode = this.getLogGroup().getSingleSelectData();
	if(angular.isUndefined(selectNode) || angular.isUndefined(selectNode.logId)){
		Messenger.post({
		    'message': &quot;请选择需要查看详情的日志！&quot;,
		    'type': 'error',
		});
	}else{
		Modal.open('f/logDetails',{type:&quot;log&quot;,data:selectNode});	
	}
}, 
backUpLog:function(){
	var _this = this;
	Modal.open('f/backupLog',{},function(result){
			if('success'==result){
		    	_this.refreshLogGroup();
		    	_this.refreshLogHistoryGroup();
		    }
		});	
},
searchLog:function(){					
	this.getLogGroup().refresh($model.searchLog);
},
resetSearchLog:function(){
	$view.log_logOperUser_Field.clean();
	$view.log_operType_Field.clean();
	$view.log_logOperdesc_Field.clean();
	$view.log_logVisitorial_Field.clean();
	$view.log_startDate_Field.clean();
	$view.log_endDate_Field.clean();
	$view.log_logStatus_Field.clean();
	$view.log_logContent_Field.clean();
},
refreshLogGroup:function(){
	this.getLogGroup().refresh();
},
// 历史日志列表Tab
getLogHistoryGroup:function(){
	return $view.logHistoryGroup;
},
logHistoryDetails:function(){
	var selectNodes = this.getLogHistoryGroup().getSelectDatas();
	if(angular.isArray(selectNodes) &amp;&amp; selectNodes.length>0){
		if(selectNodes.length>1){
			Messenger.post({
			    'message': &quot;请仅选择一个需要查看详情的历史日志！&quot;,
			    'type': 'error',
			});
			return;
		}
		var selectNode = selectNodes[0];
		if(angular.isUndefined(selectNode) || angular.isUndefined(selectNode.logId)){
			Messenger.post({
			    'message': &quot;请选择需要查看详情的历史日志！&quot;,
			    'type': 'error',
			});
		}else{
			Modal.open('f/logDetails',{type:&quot;logHistory&quot;,data:selectNode});	
		}
	}else{
		Messenger.post({
		    'message': &quot;请选择需要查看详情的历史日志！&quot;,
		    'type': 'error',
		});
	}
}, 
delLogHistory:function(){
	var selectNodes = this.getLogHistoryGroup().getSelectDatas();
	if(angular.isArray(selectNodes) &amp;&amp; selectNodes.length>0){
		var logIds = [];
		var _this = this;
		angular.forEach(selectNodes, function (value, key) {
				logIds.push(value.logId);
        	});
        Modal.openDelConfirm({message:&quot;是否确认删除历史日志?&lt;br>待删除历史日志条数：&quot;+logIds.length+&quot;条&quot;},function(){
        	$http.post(&quot;ws/delLogHistory&quot;,{&quot;logIds&quot;:logIds})
        			.success(function(data, status, headers, config){
						Messenger.post({
			                'message': &quot;删除历史日志成功！&quot;,
			                'type': 'success',
			            });
			            _this.refreshLogHistoryGroup();
					});
        });		
	}else{
		Messenger.post({
		    'message': &quot;请选择需要删除的历史日志！&quot;,
		    'type': 'error',
		});
	}
},
searchLogHistory:function(){	
	this.getLogHistoryGroup().refresh($model.searchLogHis);
},
resetSearchLogHistory:function(){
	$view.logHis_logOperUser_Field.clean();
	$view.logHis_operType_Field.clean();
	$view.logHis_logOperdesc_Field.clean();
	$view.logHis_logVisitorial_Field.clean();
	$view.logHis_startDate_Field.clean();
	$view.logHis_endDate_Field.clean();
	$view.logHis_logStatus_Field.clean();
	$view.logHis_logContent_Field.clean();
},
refreshLogHistoryGroup:function(){
	this.getLogHistoryGroup().refresh();
},
// 日志配置Tab
getLogConfigGroup:function(){
	return $view.logConfigGroup;
},
// 编辑日志配置
editLogConfig:function(){
	var selectNode = this.getLogConfigGroup().getSingleSelectData(); 
	if(angular.isUndefined(selectNode) || angular.isUndefined(selectNode.operModule)){
		Messenger.post({
		    'message': &quot;请选择需要编辑的日志配置！&quot;,
		    'type': 'error',
		});
	}else{
		var _this = this;
		Modal.open('f/editLogConfig',{operModule:selectNode.operModule},function(result){
			if('success'==result){
		    	_this.refreshLogConfigGroup();
		    }
		});	
	}	
},
searchLogConfigSub:function(){	
	this.getLogConfigGroup().refresh($model.searchLogCon);
},
resetSearchLogConfigSub:function(){
	$view.logCon_logOperdesc_Field.clean();
	$view.logCon_logType_Field.clean();
	$view.logCon_logOper_Field.clean();
	$view.logCon_logEnabled_Field.clean();
},
refreshLogConfigGroup:function(){
	this.getLogConfigGroup().refresh();
},
// 页面加载完成
page_onload:function(){
	var _this = this;
	var doitLog = debounce(function(search){
			if(_this.getLogGroup()){
				_this.getLogGroup().refresh(search);
			}
		},300,false);	
	
	var doitLogHis = debounce(function(search){
			if(_this.getLogHistoryGroup()){
				_this.getLogHistoryGroup().refresh(search);
			}
		},300,false);
	
	var doitLogCon = debounce(function(search){
			if(_this.getLogConfigGroup()){
				_this.getLogConfigGroup().refresh(search);
			}
		},300,false);
		
	$scope.$watch(&quot;$model.searchLog&quot;,function(v,o){
		if(angular.isUndefined(v)){
			return ;
		}
		doitLog(v);
	},true);	
	
	$scope.$watch(&quot;$model.searchLogHis&quot;,function(v,o){
		if(angular.isUndefined(v)){
			return ;
		}
		doitLogHis(v);
	},true);
	
	$scope.$watch(&quot;$model.searchLogCon&quot;,function(v,o){
		if(angular.isUndefined(v)){
			return ;
		}
		doitLogCon(v);
	},true);
}</form:scripts>
