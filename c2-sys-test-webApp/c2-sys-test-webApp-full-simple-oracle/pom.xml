	<name>c2-sys-test-webApp-full-simple-oracle</name>
	<description>系统管理-测试-webApp-完整精简版-oracle</description>
	<artifactId>c2-sys-test-webApp-full-simple-oracle</artifactId>
	<packaging>war</packaging>
	<dependencies>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>3.8.1</version>
			<scope>test</scope>
		</dependency>
			<dependency>
			<groupId>com.chinacreator.c2</groupId>
			<artifactId>c2-runtime</artifactId>
			<version>4.2.4</version>
		</dependency>
		<dependency>
			<groupId>com.chinacreator.c2</groupId>
			<artifactId>c2-ui-components</artifactId>
			<version>4.2.8.4</version>
		</dependency>
		<dependency>
			<groupId>com.chinacreator.c2</groupId>
			<artifactId>c2-sys-mysql</artifactId>
			<version>4.3.0-SNAPSHOT</version>
