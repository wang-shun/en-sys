  <form:datasources>
    <form:datasource xsi:type="form:Inputs"/>
    <form:datasource xsi:type="form:PagingDataSource" id="resList" name="列表型数据源" logic="com.chinacreator.asp.comp.sys.datautil.ide.web.ResDataArrayContentProviderImpl" pageSize="10" type="custom"/>
    <form:datasource xsi:type="form:TreeDataSource" id="menuTree" name="菜单树型数据源" impl="com.chinacreator.asp.comp.sys.datautil.ide.web.MenuTreeContentProviderImpl" type="custom"/>
    <form:datasource xsi:type="form:LogicDataSource" id="menu" name="菜单" logic="rule:com.chinacreator.res.getMenuByMenuRes" sn="getMenuByMenuRes">
      <form:param id="menuRes" name="新输入参数"/>
    </form:datasource>
  </form:datasources>
  <form:scripts>// 刷新资源列表&#xD;
refreshResGroup:function(){&#xD;
	$view.resGroup.refresh();&#xD;
},&#xD;
// 刷新菜单树&#xD;
refreshMenuTree:function(){&#xD;
	$view.menuTree.reAsyncChildNodes(null,&quot;refresh&quot;,false);&#xD;
	$http.post(&quot;ws/refreshMenu&quot;,{});&#xD;
},&#xD;
// 菜单树点击事件&#xD;
menuTree_nodeClick:function(event,treeId,treeNode,clickFlag){&#xD;
	$model.menu.$load({menuRes:treeNode.hiddenName});&#xD;
},&#xD;
// 同步资源&#xD;
synchroRes:function(){&#xD;
	var _this = this;&#xD;
	Modal.openConfirm({message:&quot;是否确认进行同步资源操作?&lt;br/>同步操作之间建议备份数据！&quot;},function(){&#xD;
		$http.post(&quot;ws/synchroRes&quot;,{})&#xD;
				.success(function(data, status, headers, config){&#xD;
					Messenger.post({&#xD;
		                'message': &quot;同步资源成功！&quot;,&#xD;
		                'type': 'success',&#xD;
		            });&#xD;
		            _this.refreshResGroup();&#xD;
		    	});&#xD;
    });&#xD;
},&#xD;
// 同步菜单&#xD;
synchroMenu:function(){&#xD;
	var _this = this;&#xD;
	Modal.openConfirm({message:&quot;是否确认进行同步菜单操作?&lt;br/>同步操作之间建议备份数据！&quot;},function(){&#xD;
		$http.post(&quot;ws/synchroMenu&quot;,{})&#xD;
				.success(function(data, status, headers, config){&#xD;
						Messenger.post({&#xD;
			                'message': &quot;同步菜单成功！&quot;,&#xD;
			                'type': 'success',&#xD;
			            });	&#xD;
			            _this.refreshMenuTree();&#xD;
				});&#xD;
    });&#xD;
},

</form:scripts>
</form:Form>
