  <form:scripts>// 查询&#xD;
searchSub:function(){&#xD;
	var param = {&#xD;
			host:$model.host,&#xD;
			startTime:$model.startTime,&#xD;
			lastAccessTime:$model.lastAccessTime,&#xD;
			authenticated:$model.authenticated,	&#xD;
	};&#xD;
	var isAdd = 0;&#xD;
	var userDTO = {};&#xD;
	if(!angular.isUndefined($model.userName) &amp;&amp; &quot;&quot;!=$model.userName){&#xD;
		userDTO.userName = $model.userName;&#xD;
		isAdd = 1;&#xD;
	}&#xD;
	if(!angular.isUndefined($model.userRealname) &amp;&amp; &quot;&quot;!=$model.userRealname){&#xD;
		userDTO.userRealname = $model.userRealname;&#xD;
		isAdd = 1;&#xD;
	}&#xD;
	if(isAdd>0){&#xD;
		param.userDTO = userDTO;&#xD;
	}&#xD;
	$view.onlineUserGroup.refresh(param);&#xD;
},&#xD;
// 重置&#xD;
resetSearch:function(){&#xD;
	$view.host_Field.clean();&#xD;
	$view.time_Field.clean();&#xD;
	$view.authenticated_Field.clean();&#xD;
