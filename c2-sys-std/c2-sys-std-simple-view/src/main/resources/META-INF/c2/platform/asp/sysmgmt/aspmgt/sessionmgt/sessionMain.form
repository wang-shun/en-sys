              <dync:item name="年-月-日" value="YYYY-MM-DD"/>
              <dync:item name="时:分:秒" value="HH:mm:ss"/>
              <dync:item name="时:分" value="HH:mm"/>
            </dync:render>
            <dync:children/>
            <dync:onChange>
              <template:case value="YYYY-MM-DD HH:mm">
                <template:set param="pickDate" value="true"/>
                <template:set param="pickTime" value="true"/>
                <template:set param="useMinutes" value="true"/>
                <template:set param="useSeconds" value="false"/>
              </template:case>
              <template:case value="YYYY-MM-DD HH:mm:ss">
                <template:set param="pickDate" value="true"/>
                <template:set param="pickTime" value="true"/>
                <template:set param="useMinutes" value="true"/>
                <template:set param="useSeconds" value="true"/>
              </template:case>
              <template:case value="YYYY-MM-DD">
                <template:set param="pickDate" value="true"/>
                <template:set param="pickTime" value="false"/>
                <template:set param="useMinutes" value="false"/>
                <template:set param="useSeconds" value="false"/>
              </template:case>
              <template:case value="HH:mm:ss">
                <template:set param="pickDate" value="false"/>
                <template:set param="pickTime" value="true"/>
                <template:set param="useMinutes" value="true"/>
                <template:set param="useSeconds" value="true"/>
              </template:case>
              <template:case value="HH:mm">
                <template:set param="pickDate" value="false"/>
                <template:set param="pickTime" value="true"/>
                <template:set param="useMinutes" value="true"/>
                <template:set param="useSeconds" value="false"/>
              </template:case>
            </dync:onChange>
          </dync:attribute>
        </form:attributes>
