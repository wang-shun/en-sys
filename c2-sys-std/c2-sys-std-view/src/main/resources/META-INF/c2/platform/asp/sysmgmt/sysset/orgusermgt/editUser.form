  <form:scripts>// 保存&#xD;
saveUser:function(){&#xD;
	if(this.validateForm() &amp;&amp; $view.mainForm.check() &amp;&amp; this.validateTime()){&#xD;
		var _this = this;&#xD;
		if('add'==$params.type){&#xD;
			$http.post(&quot;ws/addUser&quot;,{userDto:$model.user.result,orgId:$params.orgId,sn:999})&#xD;
					.success(function(data, status, headers, config){&#xD;
				Messenger.post({&#xD;
	                'message': &quot;新增用户成功！&quot;,&#xD;
	                'type': 'success',&#xD;
	            });&#xD;
	            $scope.saveType = 'success';&#xD;
	            _this.closeModal();&#xD;
			});&#xD;
		}else if('edit'==$params.type){&#xD;
			$http.post(&quot;ws/editUser&quot;,{userDto:$model.user.result})&#xD;
					.success(function(data, status, headers, config){&#xD;
						Messenger.post({&#xD;
			                'message': &quot;编辑用户成功！&quot;,&#xD;
			                'type': 'success',&#xD;
			            });&#xD;
			            $scope.saveType = 'success';&#xD;
			            _this.closeModal();&#xD;
					});&#xD;
		}else{&#xD;
			Messenger.post({&#xD;
	                'message': &quot;参数不正确！&quot;+$params.type,&#xD;
	                'type': 'error',&#xD;
	            });&#xD;
		}&#xD;
	}else{&#xD;
		Messenger.post({&#xD;
            'message': &quot;保存失败！验证未通过！&quot;,&#xD;
            'type': 'error',&#xD;
        });&#xD;
	}&#xD;
},&#xD;
// 重置&#xD;
resetUser:function(){&#xD;
	$view.mainForm.clean();&#xD;
	this.page_beforeDataSourceLoad();&#xD;
	$model.dictData.$reload();&#xD;
	$model.orgs.$reload();&#xD;
	$model.validataJson = {};&#xD;
},&#xD;
// 关闭&#xD;
closeModal:function(){&#xD;
	Modal.instance.close($scope.saveType);&#xD;
},&#xD;
// 验证过期时间不能大于开通时间&#xD;
validateTime:function(){&#xD;
	var dredgeTime = $model.user.result.dredgeTime;&#xD;
	var pastTime = $model.user.result.pastTime;&#xD;
	if(!angular.isUndefined(dredgeTime) &amp;&amp; !angular.isUndefined(pastTime)){&#xD;
		if(dredgeTime>pastTime){&#xD;
			$view.dredgeTime.setTipMessage(&quot;过期时间不能大于开通时间！&quot;);&#xD;
			$view.pastTime.setTipMessage(&quot;过期时间不能大于开通时间！&quot;);&#xD;
			return false;&#xD;
		}&#xD;
	}&#xD;
	return true;&#xD;
},&#xD;
// 失去焦点事件(验证唯一)&#xD;
element_blur:function($event){	&#xD;
	var elementId = $event.currentTarget.id;&#xD;
	if(!$view[elementId].check()){&#xD;
		return;&#xD;
	}	&#xD;
	var elementValue = $event.currentTarget.value;&#xD;
	var validataJson = $model.validataJson;	&#xD;
	if(angular.isUndefined(validataJson)){&#xD;
		validataJson = {};&#xD;
	}&#xD;
	$http.post(&quot;ws/validateFormByUser&quot;,{&quot;elementId&quot;:elementId,&quot;elementValue&quot;:elementValue,&quot;formType&quot;:$params.type})&#xD;
			.success(function(data, status, headers, config){&#xD;
				if(!angular.isUndefined(data) &amp;&amp; !angular.isUndefined(data.result)){&#xD;
					validataJson[elementId] = data.result.validate;&#xD;
					$model.validataJson = validataJson;&#xD;
					if(&quot;error&quot;==data.result.validate){					&#xD;
						$view[elementId].setTipMessage(data.result.errmess);&#xD;
					}			&#xD;
				}&#xD;
			});&#xD;
},&#xD;
// 验证表单(验证唯一)&#xD;
validateForm:function(){&#xD;
	var validataJson = $model.validataJson;&#xD;
	if(!angular.isUndefined(validataJson)){&#xD;
		for(var key in validataJson){  &#xD;
	        if(&quot;error&quot;==validataJson[key]){	        	&#xD;
	        	return false;&#xD;
	        }&#xD;
		}&#xD;
	}	  &#xD;
    return true;&#xD;
},

page_beforeDataSourceLoad:function(){&#xD;
	$http.post(&quot;ws/getUserByPK&quot;,{&quot;userId&quot;:$params.userId})&#xD;
		.success(function(data, status, headers, config){&#xD;
			if(!angular.isUndefined(data.result.dredgeTime)){&#xD;
				data.result.dredgeTime = parseInt(data.result.dredgeTime);&#xD;
			}&#xD;
			$model.user=data;&#xD;
		});
}</form:scripts>
