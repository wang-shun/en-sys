  <form:datasources>
    <form:datasource xsi:type="form:Inputs"/>
    <form:datasource xsi:type="form:LogicDataSource" id="menu" name="菜单" logic="rule:asp.sysmgmt.sysset.jobresmgt.getMenuByPK" sn="getMenuByPK">
      <form:param id="menuId" name="新输入参数"/>
    </form:datasource>
  </form:datasources>
  <form:scripts>// 保存&#xD;
saveMenu:function(){&#xD;
	if(this.validateForm() &amp;&amp; $view.mainForm.check()){&#xD;
		if('add'==$params.type){&#xD;
			$model.menu.result.parentId = $params.parentId;&#xD;
			$http.post(&quot;ws/addMenu&quot;,{menuDTO:$model.menu.result})&#xD;
					.success(function(data, status, headers, config){&#xD;
						Messenger.post({&#xD;
			                'message': &quot;新增菜单成功！&quot;,&#xD;
			                'type': 'success',&#xD;
			            });&#xD;
			            $scope.saveType = 'success';&#xD;
					});			&#xD;
		}else if('edit'==$params.type){&#xD;
			$http.post(&quot;ws/editMenu&quot;,{menuDTO:$model.menu.result})&#xD;
					.success(function(data, status, headers, config){&#xD;
						Messenger.post({&#xD;
			                'message': &quot;编辑菜单成功！&quot;,&#xD;
			                'type': 'success',&#xD;
			            });&#xD;
			            $scope.saveType = 'success';&#xD;
					});&#xD;
		}else{&#xD;
			Messenger.post({&#xD;
	                'message': &quot;参数不正确！&quot;+$params.type,&#xD;
	                'type': 'error',&#xD;
	            });&#xD;
		}&#xD;
	}else{&#xD;
		Messenger.post({&#xD;
            'message': &quot;保存失败！验证未通过！&quot;,&#xD;
            'type': 'error',&#xD;
        });&#xD;
	}&#xD;
},&#xD;
// 重置&#xD;
resetMenu:function(){&#xD;
	$view.mainForm.clean();&#xD;
	$model.menu.$reload();	&#xD;
	$model.validataJson = {};&#xD;
},&#xD;
// 关闭&#xD;
closeModal:function(){&#xD;
	Modal.instance.close($scope.saveType);&#xD;
},&#xD;
// 失去焦点事件(验证唯一)&#xD;
element_blur:function($event){	&#xD;
	var elementId = $event.currentTarget.id;&#xD;
	if(!$view[elementId].check()){&#xD;
		return;&#xD;
	}	&#xD;
	var elementValue = $event.currentTarget.value;&#xD;
	var validataJson = $model.validataJson;	&#xD;
	if(angular.isUndefined(validataJson)){&#xD;
		validataJson = {};&#xD;
	}&#xD;
	if('add'==$params.type){&#xD;
		$model.menu.result.parentId = $params.parentId;&#xD;
	}&#xD;
	$http.post(&quot;ws/validateFormByMenu&quot;,{&quot;elementId&quot;:elementId,&quot;elementValue&quot;:elementValue,&quot;formType&quot;:$params.type,menuDTO:$model.menu.result})&#xD;
			.success(function(data, status, headers, config){&#xD;
				if(!angular.isUndefined(data) &amp;&amp; !angular.isUndefined(data.result)){&#xD;
					validataJson[elementId] = data.result.validate;&#xD;
					$model.validataJson = validataJson;&#xD;
					if(&quot;error&quot;==data.result.validate){					&#xD;
						$view[elementId].setTipMessage(data.result.errmess);&#xD;
					}			&#xD;
				}&#xD;
			});&#xD;
},&#xD;
// 验证表单(验证唯一)&#xD;
validateForm:function(){&#xD;
	var validataJson = $model.validataJson;&#xD;
	if(!angular.isUndefined(validataJson)){&#xD;
		for(var key in validataJson){  &#xD;
	        if(&quot;error&quot;==validataJson[key]){	        	&#xD;
	        	return false;&#xD;
	        }&#xD;
		}&#xD;
	}	  &#xD;
    return true;&#xD;
}&#xD;
</form:scripts>
</form:Form>
