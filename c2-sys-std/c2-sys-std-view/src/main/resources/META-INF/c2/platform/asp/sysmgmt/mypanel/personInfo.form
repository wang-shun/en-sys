  <form:scripts>// 保存
saveUser:function(){
	if($view.mainForm.check()){
		$http.post(&quot;ws/personInfo_save&quot;,$model.user)
				.success(function(data, status, headers, config){
					Messenger.post({
		                'message': &quot;个人信息修改成功！&quot;,
		                'type': 'success',
		            });
				});
	}else{
		Messenger.post({
            'message': &quot;保存失败！验证未通过！&quot;,
            'type': 'error',
        });
	}	
},
// 重置
resetUser:function(){
	$view.mainForm.clean();
	this.page_beforeDataSourceLoad();
	$model.orgs.$reload();
	$model.dictData.$reload();
},

page_beforeDataSourceLoad:function($params){	
	$http.post(&quot;ws/getLogUser&quot;,{})
		.success(function(data, status, headers, config){
			if(!angular.isUndefined(data.result.dredgeTime)){
				data.result.dredgeTime = parseInt(data.result.dredgeTime);
			}
			$model.user=data;
		});
}</form:scripts>
